<h2 class="ui dividing header">Contracts</h2>
{{#link-to "dashboard.contracts.new"  }}
<div class="ui button">New</div>
{{/link-to}}
<div class="ui middle aligned divided list">
{{#each model as |contract|}}
<div class="item">
  <div class="right floated content">
    {{#link-to "dashboard.contracts.contract" contract }}
    <div class="ui button">View</div>
    {{/link-to}}
  </div>
  <div class="right floated content">
    <div class="ui button" >Delete</div>
  </div>
  <img class="ui avatar image" src="/images/lena.png">
  <div class="content">
    {{contract.employee.firstname}}
  </div>
</div>
{{/each}}

</div>
